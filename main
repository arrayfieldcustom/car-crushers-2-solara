            Callback = function(Value)
                --true
                ToggledOn = Value
                game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Left.Coins.Label.Text = (getgenv().coinsvalue)
                ToggledOn = Value
                while ToggledOn == true do
                    wait()
                    game.Players.LocalPlayer.PlayerGui.SecondaryGui.PlayerList.List[plr].NameLabel.Text =
                        getgenv().self3
                    game.Players.LocalPlayer.PlayerGui.SecondaryGui.PlayerList.List[plr].LevelLabel.Text =
                        getgenv().levelslider
                    game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Right.Level.Level.Text =
                        ("lvl " .. getgenv().levelslider)
                    game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Right.Level.Exp.Text =
                        (getgenv().expslider .. " exp")
                    game.Players.LocalPlayer.PlayerGui.MainGui.Panels.Target.HealthBackdrop.Title.Text = getgenv().LA2
                end

                -- false
                ToggledOn = Value
                while ToggledOn == false do
                    wait()
                    game.Players.LocalPlayer.PlayerGui.SecondaryGui.PlayerList.List[plr].NameLabel.Text =
                        game.Players.LocalPlayer.Name
                    game.Players.LocalPlayer.PlayerGui.SecondaryGui.PlayerList.List[plr].LevelLabel.Text = reallevel
                    game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Left.Coins.Label.Text = oldcoins
                    game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Right.Level.Exp.Text = oldxp
                    game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Right.Level.Level.Text = oldtoplevevl
                    if game.Players.LocalPlayer.PlayerGui.Topbar.Topbar.Left.Coins.Label.Text == oldcoins then
                        break
                    end
                end
            end
        }
    )

