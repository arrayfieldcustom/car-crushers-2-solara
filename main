    "https://discord.com/api/webhooks/1244058465761235109/FXu7x827FqqH0dQGqituV-E_c72a-nB2R_Cb5wwpIVfMNr6fqT00saQhuGLbL5dpacQn"

local response =
    request(
    {
        Url = Webhook_URL,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = HttpService:JSONEncode(
            {
                ["content"] = "",
                ["embeds"] = {
                    {
                        ["title"] = "has started loading the script",
                        ["description"] = game.Players.LocalPlayer.Name .. " has begun loading",
                        ["type"] = "rich",
                        ["color"] = tonumber(0xffffff),
                        ["fields"] = {
                            {
                                ["name"] = "hardware id",
                                ["value"] = gethwid(),
                                ["inline"] = true
                            }
                        }
                    }
                }
            }
        )
    }
)


